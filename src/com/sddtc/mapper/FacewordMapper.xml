<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sddtc.mapper.FacewordMapper">
	<select id="query" resultType="Faceword">
		SELECT * FROM t_cp_faceword
	</select>
	<insert id="add" parameterType="Faceword" useGeneratedKeys="true" keyProperty="id">
	    insert  into t_cp_faceword(text, create_time, type)
	    values (#{text}, #{create_time}, #{type})
	</insert>
</mapper>